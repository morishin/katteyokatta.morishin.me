{
  "private": true,
  "engines": {
    "node": "16"
  },
  "volta": {
    "node": "16.17.0"
  },
  "scripts": {
    "dev": "concurrently --kill-others -n next,graphql,prisma -c cyan,gray,gray 'npm run next:dev' 'npm run graphql-codegen -- --watch' 'npm run prisma:generate -- --watch'",
    "next:dev": "dotenv -e .env.development -- next dev",
    "build": "next build",
    "postinstall": "prisma generate",
    "start": "next start -p $PORT",
    "lint:eslint": "next lint --max-warnings 0",
    "lint:prettier": "prettier --list-different '**/*.{graphql,js,ts,tsx}'",
    "lint:prettier:write": "prettier --write '**/*.{graphql,js,ts,tsx}'",
    "lint:tsc": "tsc",
    "format": "prettier --list-different --write '**/*.{graphql,js,ts,tsx}'",
    "prisma:generate": "prisma generate",
    "prisma:db:push": "dotenv -e .env.development -- prisma db push",
    "prisma:migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "prisma:studio": "dotenv -e .env.development -- prisma studio",
    "prisma:seed": "dotenv -e .env.development -- tsx prisma/seed.ts",
    "graphql-codegen": "graphql-codegen --overwrite --config graphql/codegen.yml"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "1.0.3",
    "@prisma/client": "3.13.0",
    "@types/micro-cors": "^0.1.2",
    "apollo-server-micro": "^3.6.7",
    "graphql": "^16.4.0",
    "graphql-request": "^4.2.0",
    "graphql-tag": "^2.12.6",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "12.1.5",
    "next-auth": "^4.10.3",
    "prisma": "3.13.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-query": "^3.38.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-codegen/typescript-react-query": "^3.5.9",
    "@graphql-codegen/typescript-resolvers": "^2.6.1",
    "@types/node": "17.0.27",
    "@types/react": "18.0.7",
    "@types/react-dom": "18.0.0",
    "concurrently": "^7.3.0",
    "dotenv-cli": "5.1.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "tsx": "^3.9.0",
    "typescript": "4.6.3"
  }
}
