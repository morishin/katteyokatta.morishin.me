{
  "private": true,
  "engines": {
    "node": "16"
  },
  "volta": {
    "node": "16.17.0"
  },
  "scripts": {
    "dev": "concurrently --kill-others -n next,prisma -c cyan,gray 'npm run next:dev' 'npm run prisma:generate -- --watch'",
    "next:dev": "dotenv -e .env.development -- next dev",
    "build": "next build",
    "postinstall": "prisma generate",
    "start": "next start -p $PORT",
    "lint:eslint": "next lint --max-warnings 0",
    "lint:prettier": "prettier --list-different '**/*.{js,ts,tsx}'",
    "lint:prettier:write": "prettier --write '**/*.{js,ts,tsx}'",
    "lint:tsc": "tsc",
    "format": "prettier --list-different --write '**/*.{js,ts,tsx}'",
    "prisma:generate": "prisma generate",
    "prisma:db:push": "dotenv -e .env.development -- prisma db push",
    "prisma:migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "prisma:migrate:reset": "dotenv -e .env.development -- prisma migrate reset",
    "prisma:studio": "dotenv -e .env.development -- prisma studio",
    "prisma:seed": "dotenv -e .env.development -- tsx prisma/seed.ts",
    "dev:migration!": "dotenv -e .env.development -- tsx dev/migration.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.3.7",
    "@emotion/react": "^11.10.5",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.5.0",
    "@trpc/client": "^10.0.0-proxy-beta.26",
    "@trpc/next": "^10.0.0-proxy-beta.26",
    "@trpc/react-query": "^10.0.0-proxy-beta.26",
    "@trpc/server": "^10.0.0-proxy-beta.26",
    "amazon-paapi": "^1.0.7",
    "date-fns": "^2.29.3",
    "next": "^13.0.2",
    "next-auth": "^4.15.2",
    "pino-http": "^8.2.1",
    "pino-pretty": "^9.1.1",
    "prisma": "^4.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.1",
    "react-icons": "^4.6.0",
    "react-spinners": "^0.13.6",
    "react-use": "^17.4.0",
    "superjson": "^1.11.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "concurrently": "^7.3.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.2",
    "pg": "^8.8.0",
    "tsx": "^3.11.0",
    "typescript": "^4.8.4"
  }
}
